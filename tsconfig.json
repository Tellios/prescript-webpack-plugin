{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": ["es2017"],
        "strict": true,
        "sourceMap": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true
    },
    "include": ["src"],
    "typeAcquisition": {
        "enable": true
    }
}
